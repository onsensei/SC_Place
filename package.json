{
  "name": "DisasterPrevention",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "-----SECTION_GENERAL-----": "",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "-----SECTION_IOS-----": "",
    "ios": "react-native run-ios",
    "iphone:simulator:default": "react-native run-ios --simulator='iPhone SE'",
    "ipad:simulator:default": "react-native run-ios --simulator='iPad Pro (9.7-inch)'",
    "iphone:device:default": "react-native run-ios --device 'Chong’s iPhone SE'",
    "iphone:device:x": "react-native run-ios --device 'Chong’s iPhone X'",
    "ipad:device:default": "react-native run-ios --device 'Chong’s iPad Pro 9.7'",
    "-----SECTION_ANDROID-----": "",
    "android": "react-native run-android",
    "-----SECTION_TEST-----": "",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "-----SECTION_LINT-----": "",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "-----SECTION_HUSKY-----": "",
    "postinstall": "rndebugger-open",
    "prepush:original": "npm run lint && npm run test",
    "prepush": "npm run lint",
    "-----SECTION_OTHER-----": "",
    "npm:list": "Get list of globally installed packages => npm list -g --depth 0",
    "ios:list": "How to list devices available to Xcode => instruments -s devices"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-rename": "^2.4.0",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.9",
    "react-navigation-redux-helpers": "^2.0.9",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "babel-jest": "23.6.0",
    "eslint": "4.x",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.0",
    "eslint-plugin-react-native": "^3.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "23.6.0",
    "jest-enzyme": "^7.0.1",
    "metro-react-native-babel-preset": "0.51.1",
    "react-dom": "^16.7.0",
    "react-native-debugger-open": "^0.3.17",
    "react-test-renderer": "^16.6.3",
    "redux-devtools": "^3.5.0",
    "redux-saga-testing": "^1.0.5"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "enzyme-react-16-adapter-setup"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native|react-navigation)"
    ]
  }
}
